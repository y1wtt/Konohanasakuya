env:
  browser: true
  es2021: true
extends:
  - 'eslint:recommended'
  - 'plugin:@typescript-eslint/recommended'
  - 'prettier'
parser: '@typescript-eslint/parser'
parserOptions:
  ecmaVersion: 12
  project : "./tsconfig.json"
plugins:
  - '@typescript-eslint'
rules:
  "@typescript-eslint/no-unused-vars": [ "warn", { argsIgnorePattern: "^_" } ]
